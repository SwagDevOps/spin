    <div class="pt-4 pb-4">
        <a class="btn btn-outline-white"
           href="https://github.com/SwagDevOps/spin"
           role="button">
            Download
            <i class="fa fa-download ml-2"></i>
        </a>
    </div>
    <!--Copyright-->
    <div class="footer-copyright pt-3 pb-3">
        &copy;
        <%= Spin::VERSION.date.split('-')[0] %>
